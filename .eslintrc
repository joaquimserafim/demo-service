{
  "env": {
    "node": true,
    "jest": true,
    "es2020": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error"],
    "no-debugger": ["error"],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"],
    "no-unused-vars": ["error"],
    "no-prototype-builtins": "off"
  }
}